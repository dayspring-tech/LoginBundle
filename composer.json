{
    "name":         "dayspring-tech/login-bundle",
    "description":  "Form login user authentication for Symfony2, with forgot/reset password, change password.",
    "keywords":     ["login", "user authentication", "forgot password", "reset password", "change password"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":     [
        {
            "name":     "Jeffrey Wong",
            "email":    "jwong@dayspring-tech.com",
            "homepage": "http://dayspring-tech.com"
        }
    ],
    "require": {
        "php":                      ">=5.5.0",
        "symfony/framework-bundle": "^2.8.2",
        "sensio/framework-extra-bundle": "^3.0.2",
        "propel/propel-bundle": "1.5.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/symfony": "2.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Dayspring\\LoginBundle\\": ""
        },
        "exclude-from-classmap": [ "Tests/" ]
    }
}
