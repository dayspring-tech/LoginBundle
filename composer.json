{
    "name": "dayspring-tech/login-bundle",
    "description": "Form login user authentication for Symfony2, with forgot/reset password, change password.",
    "keywords": [
        "login",
        "user authentication",
        "forgot password",
        "reset password",
        "change password"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeffrey Wong",
            "email": "jwong@dayspring-tech.com",
            "homepage": "http://dayspring-tech.com"
        },
        {
            "name": "Matt Crawford",
            "email": "mcrawford@dayspring-tech.com",
            "homepage": "http://dayspring-tech.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~5.0",
        "phing/phing": "^2.12",
        "propel/propel-bundle": "1.6.x-dev",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dayspring\\LoginBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    }
}
