<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="dayspring_login.user_provider" class="Dayspring\LoginBundle\Security\User\DayspringUserProvider">
        </service>
        <service id="dayspring_login.username_password_authenticator" class="Dayspring\LoginBundle\Security\UsernamePasswordAuthenticator">
            <argument type="service" id="service_container" />
        </service>
        <service id="dayspring_login.totp_authenticator" class="Dayspring\LoginBundle\Security\TotpAuthenticator">
            <argument type="service" id="service_container" />
        </service>
        <service id="dayspring_login.totp_authenticator_helper" class="Dayspring\LoginBundle\Security\TotpAuthenticatorHelper">

        </service>
    </services>
</container>
